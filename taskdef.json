{
    "family": "financetask",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%"",
            "name": "fincecontainer",
            "cpu": 0,
            "memory": 500,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        }
    ]
}
