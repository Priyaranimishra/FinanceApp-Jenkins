        {
            "image": 716523076871.dkr.ecr.us-east-1.amazonaws.com/fianncerepo-demo:latest
            "name": "demoappcontainer",
            "cpu": 0,
            "memory": 500,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
